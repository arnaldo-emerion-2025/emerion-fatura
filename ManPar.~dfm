  object UpPar: TUpdateSQL
    ModifySQL.Strings = (
      'update FatPar'
      'set'
      '  FLGCOT = :FLGCOT,'
      '  CODTMO = :CODTMO,'
      '  TIPTMO = :TIPTMO,'
      '  DULCOT = :DULCOT,'
      '  VALCOT = :VALCOT,'
      '  PERPIS = :PERPIS,'
      '  PERCOF = :PERCOF,'
      '  QTDLIN = :QTDLIN,'
      '  QTDNOT = :QTDNOT,'
      '  CODUSU = :CODUSU,'
      '  CONPLC = :CONPLC,'
      '  SUBPLC = :SUBPLC,'
      '  LANFAT = :LANFAT,'
      '  IMPCLF = :IMPCLF,'
      '  LEROBS = :LEROBS,'
      '  IMPREF = :IMPREF,'
      '  CONSPEDXNOTA = :CONSPEDXNOTA,'
      '  CALC_PIS_ZF = :CALC_PIS_ZF,'
      '  CALC_COF_ZF = :CALC_COF_ZF,'
      '  FLG_PLAN_IMPORT = :FLG_PLAN_IMPORT,'
      '  FLG_DEL_TITULOS = :FLG_DEL_TITULOS,'
      '  EMAIL_CONTABILIDADE = :EMAIL_CONTABILIDADE,'
      '  ALIQ_CRED_SN = :ALIQ_CRED_SN,'
      '  SUPRIMIR_ZEROS = :SUPRIMIR_ZEROS'
      ' ')
    InsertSQL.Strings = (
      'insert into FatPar'
      
        '  (FLGCOT, CODTMO, TIPTMO, DULCOT, VALCOT, PERPIS, PERCOF, QTDLI' +
        'N, QTDNOT, '
      
        '   CODUSU, CONPLC, SUBPLC, LANFAT, IMPCLF, LEROBS, IMPREF, CONSP' +
        'EDXNOTA, '
      
        '   CALC_PIS_ZF, CALC_COF_ZF, FLG_PLAN_IMPORT, FLG_DEL_TITULOS, E' +
        'MAIL_CONTABILIDADE, '
      '   ALIQ_CRED_SN, SUPRIMIR_ZEROS)'
      'values'
      
        '  (:FLGCOT, :CODTMO, :TIPTMO, :DULCOT, :VALCOT, :PERPIS, :PERCOF' +
        ', :QTDLIN,'
      
        '   :QTDNOT, :CODUSU, :CONPLC, :SUBPLC, :LANFAT, :IMPCLF, :LEROBS' +
        ', :IMPREF,'
      
        '   :CONSPEDXNOTA, :CALC_PIS_ZF, :CALC_COF_ZF, :FLG_PLAN_IMPORT, ' +
        ':FLG_DEL_TITULOS,'
      '   :EMAIL_CONTABILIDADE, :ALIQ_CRED_SN, :SUPRIMIR_ZEROS)'
      ' ')
    DeleteSQL.Strings = (
      'delete from FatPar'
      'where'
      '  FLGCOT = :OLD_FLGCOT and'
      '  CODTMO = :OLD_CODTMO and'
      '  TIPTMO = :OLD_TIPTMO and'
      '  DULCOT = :OLD_DULCOT and'
      '  VALCOT = :OLD_VALCOT and'
      '  PERPIS = :OLD_PERPIS and'
      '  PERCOF = :OLD_PERCOF and'
      '  QTDLIN = :OLD_QTDLIN and'
      '  QTDNOT = :OLD_QTDNOT and'
      '  CODUSU = :OLD_CODUSU and'
      '  CONPLC = :OLD_CONPLC and'
      '  SUBPLC = :OLD_SUBPLC and'
      '  LANFAT = :OLD_LANFAT and'
      '  IMPCLF = :OLD_IMPCLF and'
      '  LEROBS = :OLD_LEROBS and'
      '  IMPREF = :OLD_IMPREF and'
      '  QTDROM = :OLD_QTDROM and'
      '  NFE2 = :OLD_NFE2 and'
      '  PATHDANFE = :OLD_PATHDANFE and'
      '  PATHCANCELAMENTO = :OLD_PATHCANCELAMENTO and'
      '  PATHNFE = :OLD_PATHNFE and'
      '  PATHXML = :OLD_PATHXML and'
      '  PATHSCHEMAS = :OLD_PATHSCHEMAS and'
