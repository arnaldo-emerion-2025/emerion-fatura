  object CmpPe2: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select CmpR04.NumPed,'
      '           CmpR04.DtePed,'
      '           CmpR04.DpePed,'
      '           EstPro.DscPro,'
      '           CmpR04.QtpPe2,'
      '           CmpR04.QtrPe2,'
      '           CmpR04.SldPe2,'
      '           CmpR04.VluPe2,'
      '           CmpR04.CodUnd,'
      '           CmpR04.IpiPe2,'
      '           CmpR04.IcmPe2,'
      '           EstPro.CodAnt,'
      '           CmpR04.DscPe2,'
      '           CmpR04.PacPe2,'
      '           GerUsu.LogUsu,'
      '           CmpR04.QtdCmp,'
      '           CmpR04.DulCmp,'
      '           CmpR04.QtdEst,'
      '           Round(CmpR04.SldPe2 * CmpR04.VluPe2,4) as TotNat,'
      
        '           TextoOcor(IntStrZeros(CmpR04.CodFor,5),'#39'-'#39',FinFor.Ape' +
        'For,'#39#39','#39#39','#39#39','#39#39') as ApeFor,'
      
        '           CmpR04.CodClp || _UNICODE_FSS '#39'-'#39' || CmpR04.CodGru ||' +
        ' _UNICODE_FSS '#39'.'#39' || CmpR04.CodSub || _UNICODE_FSS '#39'.'#39' || CmpR04' +
        '.CodPro as CodIte'
      'From CmpR04 LEFT JOIN EstPro ON (CmpR04.CodClp = EstPro.CodPro)'
      
        '                                                   AND (CmpR04.C' +
        'odGru = EstPro.CodGru)'
      
        '                                                   AND (CmpR04.C' +
        'odSub = EstPro.CodSub)'
      
        '                                                   AND (CmpR04.C' +
        'odPro = EstPro.CodPro)'
      
        '                        LEFT JOIN FinFor ON (CmpR04.CodFor = Fin' +
        'For.CodFor)'
      
        '                      LEFT JOIN GerUsu ON (CmpR04.CodUsu = GerUs' +
        'u.CodUsu)'
      'Where CmpR04.SeqArq = :SeqArq'
      'Order by CmpR04.CodGru,CmpR04.CodSub,CmpR04.CodPro,CmpR04.NumPed')
    ValidateWithMask = True
    Left = 104
    Top = 53
    ParamData = <
      item
        DataType = ftString
        Name = 'SeqArq'
        ParamType = ptInput
      end>
