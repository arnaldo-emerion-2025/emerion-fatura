  object perfilDevolucao: TwwQuery
    Tag = 99
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'select'
      
        '   dv2.codgru || '#39'.'#39' || dv2.codsub || '#39'.'#39' || dv2.codpro as CODIG' +
        'O,'
      '   dv2.desdv2 as DESCRICAO,'
      '   dev.codcli as COD_CLIENTE,'
      '   dev.numres as NUM_PEDIDO,'
      '   dev.numnfs as NFE_VENDA,'
      '   dev.nfscli as NFE_CLIENTE,'
      '   ger.nronfs as NFE_ENTRADA,'
      '   dev.dtedev as DATA_EMISSAO_DEVOLUCAO,'
      '   dv2.qtpdv2 as QTD_PEDIDO_VENDA,'
      '   dv2.ultqtd as QTD_DEVOLVIDA,'
      '   dev.sitdev'
      ''
      'from fatdv2 dv2'
      'left join fatdev dev on'
      '                    dv2.codemp = dev.codemp and'
      '                    dv2.dteres = dev.dteres and'
      '                    dv2.numres = dev.numres and'
      '                    dv2.seqlib = dev.seqlib and'
      '                    dv2.seqfat = dev.seqfat and'
      '                    dv2.seqdev = dev.seqdev'
      'left join fatger ger on'
      '                    ger.id_fatdev   =   dev.id_fatdev   and'
      '                    ger.codemp      =   dev.codemp      and'
      '                    ger.numger      =   dev.numger'
      'where 1 = 2')
    ValidateWithMask = True
    Left = 657
    Top = 34
    object perfilDevolucaoCODIGO: TStringField
      FieldName = 'CODIGO'
      Size = 14
    end
    object perfilDevolucaoDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Size = 70
    end
    object perfilDevolucaoCOD_CLIENTE: TIntegerField
      DisplayWidth = 5
      FieldName = 'COD_CLIENTE'
    end
    object perfilDevolucaoNUM_PEDIDO: TIntegerField
      FieldName = 'NUM_PEDIDO'
    end
    object perfilDevolucaoNFE_VENDA: TIntegerField
      FieldName = 'NFE_VENDA'
    end
    object perfilDevolucaoNFE_CLIENTE: TIntegerField
      FieldName = 'NFE_CLIENTE'
    end
    object perfilDevolucaoNFE_ENTRADA: TIntegerField
      FieldName = 'NFE_ENTRADA'
    end
    object perfilDevolucaoDATA_EMISSAO_DEVOLUCAO: TDateTimeField
      FieldName = 'DATA_EMISSAO_DEVOLUCAO'
    end
    object perfilDevolucaoQTD_PEDIDO_VENDA: TFloatField
      FieldName = 'QTD_PEDIDO_VENDA'
    end
    object perfilDevolucaoQTD_DEVOLVIDA: TFloatField
      FieldName = 'QTD_DEVOLVIDA'
    end
    object perfilDevolucaoSITDEV: TStringField
      FieldName = 'SITDEV'
      Size = 45
    end
  end
