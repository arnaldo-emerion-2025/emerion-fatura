    end
    object SQLBUSDTEFAT: TDateTimeField
      FieldName = 'DTEFAT'
    end
    object SQLBUSSEQNFE: TStringField
      FieldName = 'SEQNFE'
      FixedChar = True
      Size = 44
    end
    object SQLBUSID: TIntegerField
      FieldName = 'ID'
    end
  end
