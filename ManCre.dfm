  object SeqRel: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select FatCre.*,'
      
        '           TextoOcor(IntStrZeros(FatCre.CodEmp,3),'#39'-'#39',GerEmp.Ape' +
        'Emp,'#39#39','#39#39','#39#39','#39#39') as ApeEmp,'
      
        '           TextoOcor(IntStrZeros(FatCre.CodCli,7),'#39'-'#39',FinCli.Nom' +
        'Cli,'#39#39','#39#39','#39#39','#39#39') as NomCli,'
      
        '           TextoOcor(IntStrZeros(FatCre.CodVen,7),'#39'-'#39',FinVen.Ape' +
        'Ven,'#39#39','#39#39','#39#39','#39#39') as ApeVen,'
      '           GerUsu.LogUsu,'
      '           FinCli.Inscli,'
      '           FinCli.CgcCli'
      'From FatCre LEFT JOIN PedRes ON (FatCre.CodEmp = PedRes.CodEmp)'
      
        '                                                  AND (FatCre.Dt' +
        'eRes = PedRes.DteRes)'
      
        '                                                  AND (FatCre.Nu' +
        'mRes = PedRes.NumRes)'
      
        '                        LEFT JOIN FinCli ON (FatCre.CodCli = Fin' +
        'Cli.CodCli)'
      
        '                        LEFT JOIN FinVen ON (FatCre.CodVen = Fin' +
        'Ven.CodVen)'
      
        '                       LEFT JOIN GerUsu ON (FatCre.CodUsu = GerU' +
        'su.CodUsu)'
      
        '                       LEFT JOIN GerEmp ON (FatCre.CodEmp = GerE' +
        'mp.CodEmp)'
      'Where FatCre.CodEmp = :CodEmp'
      '     and FatCre.DteRes = :DteRes'
      '     and FatCre.NumRes = :NumRes'
      '     and FatCre.SeqCre = :SeqCre'
      
        'Order by FatCre.DteCre,FatCre.CodEmp,FatCre.DteRes,FatCre.NumRes' +
        ',FatCre.SeqCre')
    ValidateWithMask = True
    Left = 240
    Top = 301
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CodEmp'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DteRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NumRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqCre'
        ParamType = ptInput
      end>
