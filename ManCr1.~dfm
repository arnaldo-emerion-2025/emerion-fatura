    object FatCreTOTCRE: TFloatField
      FieldName = 'TOTCRE'
      DisplayFormat = '###,###,##0.00'
      Precision = 2
    end
    object FatCreMEDCOM: TFloatField
      FieldName = 'MEDCOM'
      DisplayFormat = '###,###,##0.00'
