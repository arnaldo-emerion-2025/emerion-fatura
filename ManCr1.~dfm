  object FatCre: TwwQuery
    Active = True
    CachedUpdates = True
    OnNewRecord = FatCreNewRecord
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select FatCre.*,'
      '           FinCli.NomCli,'
      '           FinVen.ApeVen,'
      '           GerUsu.LogUsu,'
      '           FinCli.Inscli,'
      '           FinCli.CgcCli'
      'From FatCre LEFT JOIN PedRes ON (FatCre.CodEmp = PedRes.CodEmp)'
      
        '                                                  AND (FatCre.Dt' +
        'eRes = PedRes.DteRes)'
      
        '                                                  AND (FatCre.Nu' +
        'mRes = PedRes.NumRes)'
      
        '                        LEFT JOIN FinCli ON (FatCre.CodCli = Fin' +
        'Cli.CodCli)'
      
        '                        LEFT JOIN FinVen ON (FatCre.CodVen = Fin' +
        'Ven.CodVen)'
      
        '                       LEFT JOIN GerUsu ON (FatCre.CodUsu = GerU' +
        'su.CodUsu)'
      'Where FatCre.CodEmp = :CodEmp'
      '     and FatCre.DteRes = :DteRes'
      '     and FatCre.NumRes = :NumRes'
      '     and FatCre.SeqCre = :SeqCre'
      
        'Order by FatCre.DteCre,FatCre.CodEmp,FatCre.DteRes,FatCre.NumRes' +
        ',FatCre.SeqCre')
    UpdateObject = UpCre
    ValidateWithMask = True
    Left = 6
    Top = 184
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CodEmp'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DteRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NumRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqCre'
        ParamType = ptInput
      end>
    object FatCreCODEMP: TIntegerField
      FieldName = 'CODEMP'
    end
    object FatCreDTERES: TDateTimeField
      DisplayWidth = 10
      FieldName = 'DTERES'
      EditMask = '!99/99/9999;1;_'
