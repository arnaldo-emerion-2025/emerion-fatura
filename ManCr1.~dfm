  object UpCre: TUpdateSQL
    ModifySQL.Strings = (
      'update FatCre'
      'set'
      '  CODCLI = :CODCLI,'
      '  CODVEN = :CODVEN,'
      '  NUMNFS = :NUMNFS,'
      '  DTECRE = :DTECRE,'
      '  HRECRE = :HRECRE,'
      '  TOTCRE = :TOTCRE,'
      '  MEDCOM = :MEDCOM,'
      '  BASCOM = :BASCOM,'
      '  TOTCOM = :TOTCOM,'
      '  OBSCRE = :OBSCRE,'
      '  CODUSU = :CODUSU,'
      '  SITCRE = :SITCRE,'
      '  DTEATU = :DTEATU,'
      '  HREATU = :HREATU,'
      '  USUATU = :USUATU'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQCRE = :OLD_SEQCRE')
    InsertSQL.Strings = (
      'insert into FatCre'
      
        '  (CODEMP, DTERES, NUMRES, SEQCRE, CODCLI, CODVEN, NUMNFS, DTECR' +
        'E, '
      'HRECRE, '
      
        '   TOTCRE, MEDCOM, BASCOM, TOTCOM, OBSCRE, CODUSU, SITCRE, DTEAT' +
        'U, '
      'HREATU, '
      '   USUATU)'
      'values'
      
        '  (:CODEMP, :DTERES, :NUMRES, :SEQCRE, :CODCLI, :CODVEN, :NUMNFS' +
        ', '
      ':DTECRE, '
      
        '   :HRECRE, :TOTCRE, :MEDCOM, :BASCOM, :TOTCOM, :OBSCRE, :CODUSU' +
        ', '
      ':SITCRE, '
      '   :DTEATU, :HREATU, :USUATU)')
    DeleteSQL.Strings = (
      'delete from FatCre'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQCRE = :OLD_SEQCRE')
    Left = 6
    Top = 212
  end
