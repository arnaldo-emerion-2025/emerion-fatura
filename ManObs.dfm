  object UpObs: TUpdateSQL
    ModifySQL.Strings = (
      'update FatObs'
      'set'
      '  NUMNFS = :NUMNFS,'
      '  DTEOBS = :DTEOBS,'
      '  HREOBS = :HREOBS,'
      '  OBSOBS = :OBSOBS,'
      '  CODUSU = :CODUSU,'
      '  SITOBS = :SITOBS,'
      '  DTEATU = :DTEATU,'
      '  HREATU = :HREATU,'
      '  USUATU = :USUATU'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQOBS = :OLD_SEQOBS')
    InsertSQL.Strings = (
      'insert into FatObs'
      
        '(CODEMP, DTERES, NUMRES, SEQOBS, NUMNFS, DTEOBS, HREOBS, OBSOBS,' +
        ' '
      'CODUSU, SITOBS, DTEATU, HREATU, USUATU)'
      'values'
      '(:CODEMP, :DTERES, :NUMRES, :SEQOBS, :NUMNFS, :DTEOBS, :HREOBS, '
      ':OBSOBS, :CODUSU, :SITOBS, :DTEATU, :HREATU, :USUATU)')
    DeleteSQL.Strings = (
      'delete from FatObs'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQOBS = :OLD_SEQOBS')
    Left = 36
    Top = 236
  end
