  object EstR02: TwwQuery
    Tag = 1
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select EstIte.CodEmp,'
      '           GerEmp.NomEmp,'
      '           EstGru.NomGru,'
      '           EstSub.NomSub,'
      '           EstPro.DscPro,'
      '           EstPro.RefPro,'
      '           EstPro.CodUns,'
      '           EstIte.QtdRma,'
      '           EstIte.QtrIte,'
      '           EstIte.QtdIte,'
      '           EstIte.QtmIte,'
      '           EstIte.QtaIte,'
      '           EstIte.QmaIte,'
      
        '           EstIte.CodClp || _UNICODE_FSS '#39'-'#39' || EstIte.CodGru ||' +
        ' _UNICODE_FSS '#39'.'#39' || EstIte.CodSub || _UNICODE_FSS '#39'.'#39' || EstIte' +
        '.CodPro as CodIte'
      'From EstIte LEFT JOIN EstPro ON (EstIte.CodClp = EstPro.CodClp'
      
        '                                              AND  EstIte.CodGru' +
        ' = EstPro.CodGru'
      
        '                                              AND  EstIte.CodSub' +
        ' = EstPro.CodSub'
      
        '                                              AND  EstIte.CodPro' +
        ' = EstPro.CodPro)'
      
        '                 LEFT JOIN EstSub ON (EstPro.CodGru = EstSub.Cod' +
        'Gru'
      
        '                                              AND  EstPro.CodSub' +
        ' = EstSub.CodSub)'
      
        '                  LEFT JOIN EstGru ON (EstSub.CodGru = EstGru.Co' +
        'dGru)'
      
        '                LEFT JOIN GerEmp ON (EstIte.CodEmp = GerEmp.CodE' +
        'mp)'
      'Where EstIte.CodGru = :CodGru'
      '     and EstIte.CodSub = :CodSub')
    ValidateWithMask = True
    Left = 280
    Top = 317
    ParamData = <
      item
        DataType = ftString
        Name = 'CodGru'
        ParamType = ptInput
      end
      item
        DataType = ftString
        Name = 'CodSub'
        ParamType = ptInput
      end>
