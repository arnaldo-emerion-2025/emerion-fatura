                while qtdnfis <= fmmandn1_nfe.FatDevQtdNfs.Value do
                begin

                  with quSQL, SQL do
                  begin

                    Close;
                    Text := ' Select Count(*) as QtdReg From FatDv2' +
                      ' Where FatDv2.CodEmp = :CodEmp' +
