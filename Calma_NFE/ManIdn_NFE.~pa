                      Params[1].AsDateTime := fmmandn1_nfe.FatDevDteRes.Value;
                      Params[2].AsInteger := fmmandn1_nfe.FatDevNumRes.Value;
                      Params[3].AsInteger := fmmandn1_nfe.FatDevSeqLib.Value;
