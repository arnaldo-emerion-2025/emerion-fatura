  object FatArq: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select FatPed.CodEmp,'
      '           FatPed.DteRes,'
      '           FatPed.NumRes,'
      '           FatPed.SeqLib,'
      '           FatPed.SeqFat,'
      '           FatPed.ArqNfe,'
      '           FatPed.FlgAtu,'
      '           FatPed.NFETH,'
      '           FatPed.ARQNFECANC,'
      '           FatPed.NFETHCANC'
      ''
      'From FatPed'
      'Where FatPed.CodEmp = :CodEmp'
      '     and FatPed.DteRes = :DteRes'
      '     and FatPed.NumRes = :NumRes'
      '     and FatPed.SeqLib = :SeqLib'
      '     and FatPed.SeqFat = :SeqFat')
    UpdateObject = UpFatArq
    ValidateWithMask = True
    Left = 65
    Top = 192
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CodEmp'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DteRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NumRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqLib'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqFat'
        ParamType = ptInput
      end>
    object FatArqARQNFE: TBlobField
      FieldName = 'ARQNFE'
      BlobType = ftBlob
      Size = 1
    end
    object FatArqFLGATU: TStringField
      FieldName = 'FLGATU'
      FixedChar = True
      Size = 1
    end
    object FatArqCODEMP: TIntegerField
      FieldName = 'CODEMP'
    end
    object FatArqDTERES: TDateTimeField
      FieldName = 'DTERES'
    end
    object FatArqNUMRES: TIntegerField
      FieldName = 'NUMRES'
    end
    object FatArqSEQLIB: TIntegerField
      FieldName = 'SEQLIB'
    end
    object FatArqSEQFAT: TIntegerField
      FieldName = 'SEQFAT'
    end
    object FatArqNFETH: TSmallintField
      FieldName = 'NFETH'
    end
    object FatArqARQNFECANC: TBlobField
      FieldName = 'ARQNFECANC'
      BlobType = ftBlob
      Size = 1
    end
    object FatArqNFETHCANC: TSmallintField
      FieldName = 'NFETHCANC'
    end
  end
  object UpFatArq: TUpdateSQL
    ModifySQL.Strings = (
      'update FatPed'
      'set'
      '  ARQNFE = :ARQNFE,'
      '  FLGATU = :FLGATU,'
      '  NFETH = :NFETH,'
      '  ARQNFECANC = :ARQNFECANC,'
      '  NFETHCANC = :NFETHCANC'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    InsertSQL.Strings = (
      'insert into FatPed'
      '  (CODEMP, DTERES, NUMRES, SEQLIB, SEQFAT, ARQNFE, FLGATU)'
      'values'
      
        '  (:CODEMP, :DTERES, :NUMRES, :SEQLIB, :SEQFAT, :ARQNFE, :FLGATU' +
        ')')
    DeleteSQL.Strings = (
      'delete from FatPed'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    Left = 93
    Top = 192
  end
  object UpFatPe2: TUpdateSQL
    ModifySQL.Strings = (
      'update FatPe2'
      'set'
      '  CODCFO = :CODCFO,'
      '  CLSIPI = :CLSIPI,'
      '  CODST1 = :CODST1,'
      '  CODST2 = :CODST2,'
      '  CODUND = :CODUND,'
      'despe2 = :despe2,'
      'obspe2 = :obspe2'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT and'
      '  SEQPE2 = :OLD_SEQPE2')
    InsertSQL.Strings = (
      'insert into FatPe2'
      
        '(CODEMP, DTERES, NUMRES, SEQLIB, SEQFAT, SEQPE2, DESPE2, OBSPE2,' +
        ' '
      'QTPPE2, VLQPE2, IPIPE2, ICMPE2, TOTPE2, TOTIPI, CODCFO, CODST1, '
      'CODST2, CLSIPI, CODUND, NROPE2)'
      'values'
      '(:CODEMP, :DTERES, :NUMRES, :SEQLIB, :SEQFAT, :SEQPE2, :DESPE2, '
      
        ':OBSPE2, :QTPPE2, :VLQPE2, :IPIPE2, :ICMPE2, :TOTPE2, :TOTIPI, :' +
        'CODCFO, '
      ':CODST1, :CODST2, :CLSIPI, :CODUND, :NROPE2)')
    DeleteSQL.Strings = (
      'delete from FatPe2'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT and'
      '  SEQPE2 = :OLD_SEQPE2')
    Left = 5
    Top = 384
  end
