  object UpFatPed: TUpdateSQL
    ModifySQL.Strings = (
      'update FatPed'
      'set'
      '  CODCF1 = :CODCF1,'
      '  CODCF2 = :CODCF2,'
      '  ESPFAT = :ESPFAT,'
      '  MARFAT = :MARFAT,'
      '  DESNAT = :DESNAT,'
      '  INSSUB = :INSSUB,'
      '  CODTRA = :CODTRA,'
      '  TRASDA = :TRASDA,'
      '  NOMTRA = :NOMTRA,'
      '  CGCTRA = :CGCTRA,'
      '  INSTRA = :INSTRA,'
      '  TENTRA = :TENTRA,'
      '  ENDTRA = :ENDTRA,'
      '  REFTRA = :REFTRA,'
      '  NUMTRA = :NUMTRA,'
      '  BAITRA = :BAITRA,'
      '  CIDTRA = :CIDTRA,'
      '  UFETRA = :UFETRA,'
      '  CEPTRA = :CEPTRA,'
      '  NROFAT = :NROFAT,'
      '  PLCTRA = :PLCTRA,'
      '  UFEPLC = :UFEPLC,'
      '  TEFCLI = :TEFCLI,'
      '  ENFCLI = :ENFCLI,'
      '  RFFCLI = :RFFCLI,'
      '  NRFCLI = :NRFCLI,'
      '  BAFCLI = :BAFCLI,'
      '  CIFCLI = :CIFCLI,'
      '  UFFCLI = :UFFCLI,'
      '  CEFCLI = :CEFCLI,'
      '  TENCLI = :TENCLI,'
      '  ENDCLI = :ENDCLI,'
      '  REFCLI = :REFCLI,'
      '  NUMCLI = :NUMCLI,'
      '  BAICLI = :BAICLI,'
      '  CIDCLI = :CIDCLI,'
      '  UFECLI = :UFECLI,'
      '  CEPCLI = :CEPCLI,'
      '  CGECLI = :CGECLI,'
      '  INECLI = :INECLI,'
      '  INFLIQ = :INFLIQ,'
      '  INFBRT = :INFBRT,'
      '  ALTVOL = :ALTVOL,'
      '  LOTNFE = :LOTNFE,'
      '  ENVNFE = :ENVNFE,'
      '  SEQNFE = :SEQNFE,'
      '  DTENFE = :DTENFE,'
      '  RECNFE = :RECNFE,'
      '  PRONFE = :PRONFE,'
      '  DOPNFE = :DOPNFE,'
      '  HRENFE = :HRENFE,'
      '  USUNFE = :USUNFE,'
      '  DTEPNF = :DTEPNF,'
      '  HREPNF = :HREPNF,'
      '  IMPNFE = :IMPNFE,'
      '  RETNFE = :RETNFE,'
      '  FLGATU = :FLGATU,'
      '  ID_FINUFF = :ID_FINUFF,'
      '  ID_FINCIF = :ID_FINCIF,'
      '  ID_FINUFE = :ID_FINUFE,'
      '  ID_FINCIE = :ID_FINCIE,'
      '  ID_TRAUFE = :ID_TRAUFE,'
      '  ID_TRACIE = :ID_TRACIE,'
      '  NFEPIS = :NFEPIS,'
      '  NFECOF = :NFECOF,'
      '  ID_ESTSIP = :ID_ESTSIP,'
      '  FLGDENEGADA = :FLGDENEGADA,'
      '  FLGIMP = :FLGIMP,'
      '  SITFAT = :SITFAT,'
      '  ENVDPEC = :ENVDPEC,'
      '  USUDPEC = :USUDPEC,'
      '  JUSTDPEC = :JUSTDPEC,'
      '  PROTDPEC = :PROTDPEC,'
      '  LOCEMB = :LOCEMB,'
      '  UFEMB = :UFEMB'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    InsertSQL.Strings = (
      'insert into FatPed'
      
        '  (CODCF1, CODCF2, ESPFAT, MARFAT, DESNAT, INSSUB, CODTRA, TRASD' +
        'A, NOMTRA, '
      
        '   CGCTRA, INSTRA, TENTRA, ENDTRA, REFTRA, NUMTRA, BAITRA, CIDTR' +
        'A, UFETRA, '
      
        '   CEPTRA, NROFAT, PLCTRA, UFEPLC, TEFCLI, ENFCLI, RFFCLI, NRFCL' +
        'I, BAFCLI, '
      
        '   CIFCLI, UFFCLI, CEFCLI, TENCLI, ENDCLI, REFCLI, NUMCLI, BAICL' +
        'I, CIDCLI, '
      
        '   UFECLI, CEPCLI, CGECLI, INECLI, INFLIQ, INFBRT, ALTVOL, LOTNF' +
        'E, ENVNFE, '
      
        '   SEQNFE, DTENFE, RECNFE, PRONFE, DOPNFE, HRENFE, USUNFE, DTEPN' +
        'F, HREPNF, '
      
        '   IMPNFE, RETNFE, FLGATU, ID_FINUFF, ID_FINCIF, ID_FINUFE, ID_F' +
        'INCIE, '
      
        '   ID_TRAUFE, ID_TRACIE, NFEPIS, NFECOF, ID_ESTSIP, FLGDENEGADA,' +
        ' FLGIMP, '
      '   SITFAT, ENVDPEC, USUDPEC, JUSTDPEC, PROTDPEC, LOCEMB, UFEMB)'
      'values'
      
        '  (:CODCF1, :CODCF2, :ESPFAT, :MARFAT, :DESNAT, :INSSUB, :CODTRA' +
        ', :TRASDA, '
      
        '   :NOMTRA, :CGCTRA, :INSTRA, :TENTRA, :ENDTRA, :REFTRA, :NUMTRA' +
        ', :BAITRA, '
      
        '   :CIDTRA, :UFETRA, :CEPTRA, :NROFAT, :PLCTRA, :UFEPLC, :TEFCLI' +
        ', :ENFCLI, '
      
        '   :RFFCLI, :NRFCLI, :BAFCLI, :CIFCLI, :UFFCLI, :CEFCLI, :TENCLI' +
        ', :ENDCLI, '
      
        '   :REFCLI, :NUMCLI, :BAICLI, :CIDCLI, :UFECLI, :CEPCLI, :CGECLI' +
        ', :INECLI, '
      
        '   :INFLIQ, :INFBRT, :ALTVOL, :LOTNFE, :ENVNFE, :SEQNFE, :DTENFE' +
        ', :RECNFE, '
      
        '   :PRONFE, :DOPNFE, :HRENFE, :USUNFE, :DTEPNF, :HREPNF, :IMPNFE' +
        ', :RETNFE, '
      
        '   :FLGATU, :ID_FINUFF, :ID_FINCIF, :ID_FINUFE, :ID_FINCIE, :ID_' +
        'TRAUFE, '
      
        '   :ID_TRACIE, :NFEPIS, :NFECOF, :ID_ESTSIP, :FLGDENEGADA, :FLGI' +
        'MP, :SITFAT, '
      '   :ENVDPEC, :USUDPEC, :JUSTDPEC, :PROTDPEC, :LOCEMB, :UFEMB)')
    DeleteSQL.Strings = (
      'delete from FatPed'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    Left = 125
    Top = 96
  end
