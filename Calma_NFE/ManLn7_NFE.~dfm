  object FatPe2: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    DataSource = DsFatPed
    SQL.Strings = (
      'Select FatPe2.CodEmp,'
      '           FatPe2.DteRes,'
      '           FatPe2.NumRes,'
      '           FatPe2.SeqLib,'
      '           FatPe2.SeqFat,'
      '           FatPe2.SeqPe2,'
      '           FatPe2.DesPe2,'
      '           FatPe2.ObsPe2,'
      '           FatPe2.QtpPe2,'
      '           FatPe2.VlqPe2,'
      '           FatPe2.IpiPe2,'
      '           FatPe2.IcmPe2,'
      '           FatPe2.TotPe2,'
      '           FatPe2.TotIpi,'
      '           FatPe2.CodCfo,'
      '           FatPe2.CodSt1,'
      '           FatPe2.CodSt2,'
      '           FatPe2.ClsIpi,'
      '           FatPe2.CodUnd,'
      '           FatPe2.NroPe2,'
      'FatPe2.TOTIBPT,'
      
        '           FatPe2.CodClp || _UNICODE_FSS '#39'-'#39' || FatPe2.CodGru ||' +
        ' _UNICODE_FSS '#39'.'#39' || FatPe2.CodSub || _UNICODE_FSS '#39'.'#39' || FatPe2' +
        '.CodPro as CodIte'
      'From FatPe2'
      'Where FatPe2.CodEmp = :CodEmp'
      '     and FatPe2.DteRes = :DteRes'
      '     and FatPe2.NumRes = :NumRes'
      '     and FatPe2.SeqLib = :SeqLib'
      '     and FatPe2.SeqFat = :SeqFat'
      'Order by FatPe2.NroPe2')
    UpdateObject = UpFatPe2
    ValidateWithMask = True
    Left = 5
    Top = 356
    ParamData = <
