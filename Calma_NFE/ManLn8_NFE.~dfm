  object FatPe2: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    DataSource = DsFatPed
    SQL.Strings = (
      'Select FatPe2.DesPe2,'
      '           FatPe2.QtpPe2,'
      '           FatPe2.VluPe2,'
      '           FatPe2.IpiPe2,'
      '           FatPe2.IcmPe2,'
      '           FatPe2.TotPe2,'
      '           FatPe2.TotIpi,'
      '           FatPe2.CodCfo,'
      '           FatPe2.ClsIpi,'
      '           FatPe2.CodUnd,'
      
        '           FatPe2.CodSt1 || _UNICODE_FSS '#39' '#39' || FatPe2.CodSt2 as' +
        ' CodSit,'
      
        '           FatPe2.CodClp || _UNICODE_FSS '#39'-'#39' || FatPe2.CodGru ||' +
        ' _UNICODE_FSS '#39'.'#39' || FatPe2.CodSub || _UNICODE_FSS '#39'.'#39' || FatPe2' +
        '.CodPro as CodIte'
      'From FatPe2'
      'Where FatPe2.CodEmp = :CodEmp'
      '     and FatPe2.DteRes = :DteRes'
      '     and FatPe2.NumRes = :NumRes'
      '     and FatPe2.SeqLib = :SeqLib'
      '     and FatPe2.SeqFat = :SeqFat'
      'Order by FatPe2.NroPe2')
    UniDirectional = True
    ValidateWithMask = True
    Left = 6
    Top = 402
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CODEMP'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DTERES'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NUMRES'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SEQLIB'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SEQFAT'
        ParamType = ptInput
      end>
