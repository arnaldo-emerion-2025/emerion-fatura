  object UpFatPed: TUpdateSQL
    ModifySQL.Strings = (
      'update FatPed'
      'set'
      '  IMPNFE = :IMPNFE,'
      '  RETNFE = :RETNFE,'
      '  DTECNE = :DTECNE,'
      '  HRECNE = :HRECNE,'
      '  FLGATU = :FLGATU'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    InsertSQL.Strings = (
      'insert into FatPed'
      
        '  (CODEMP, DTERES, NUMRES, SEQLIB, SEQFAT, DTEFAT, NRONFS, CODCL' +
        'I, '
      'CODPFA, '
      
        '   CODCF1, TENCLI, ENDCLI, NUMCLI, REFCLI, BAICLI, CIDCLI, UFECL' +
        'I, BASICM, '
      
        '   TOTICM, BASSUB, TOTSUB, BASIPI, TOTIPI, TOTFAT, TOTGER, IMPNF' +
        'E, '
      'SEQNFE, '
      
        '   RECNFE, PRONFE, PRCNFE, DTEPNF, HREPNF, DTCNFE, HRCNFE, RETNF' +
        'E, '
      'DTECNE, '
      '   HRECNE, FLGATU, SITFAT)'
      'values'
      
        '  (:CODEMP, :DTERES, :NUMRES, :SEQLIB, :SEQFAT, :DTEFAT, :NRONFS' +
        ', '
      ':CODCLI, '
      
        '   :CODPFA, :CODCF1, :TENCLI, :ENDCLI, :NUMCLI, :REFCLI, :BAICLI' +
        ', :CIDCLI, '
      
        '   :UFECLI, :BASICM, :TOTICM, :BASSUB, :TOTSUB, :BASIPI, :TOTIPI' +
        ', :TOTFAT, '
      
        '   :TOTGER, :IMPNFE, :SEQNFE, :RECNFE, :PRONFE, :PRCNFE, :DTEPNF' +
        ', '
      ':HREPNF, '
      
        '   :DTCNFE, :HRCNFE, :RETNFE, :DTECNE, :HRECNE, :FLGATU, :SITFAT' +
        ')')
    DeleteSQL.Strings = (
      'delete from FatPed'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    Left = 6
    Top = 275
  end
