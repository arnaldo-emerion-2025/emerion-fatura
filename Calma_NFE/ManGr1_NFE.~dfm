      '  ENVDPEC = :ENVDPEC,'
      '  USUDPEC = :USUDPEC,'
      '  JUSTDPEC = :JUSTDPEC,'
      '  PROTDPEC = :PROTDPEC'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTEGER = :OLD_DTEGER and'
      '  NUMGER = :OLD_NUMGER')
    InsertSQL.Strings = (
      'insert into FatGer'
      
        '  (ENVNFE, CODCF1, CODCF2, FLGSAI, FLGENT, TIPFRT, ESPGER, MARGE' +
        'R, DESNAT, '
      
        '   INSSUB, CODTRA, NOMTRA, CGCTRA, INSTRA, TENTRA, ENDTRA, REFTR' +
        'A, NUMTRA, '
      
        '   BAITRA, CIDTRA, UFETRA, CEPTRA, NROGER, PLCTRA, UFEPLC, CEFCL' +
        'I, TEFCLI, '
      
        '   ENFCLI, RFFCLI, NRFCLI, BAFCLI, CIFCLI, UFFCLI, CEPCLI, TENCL' +
        'I, ENDCLI, '
      
        '   REFCLI, NUMCLI, BAICLI, CIDCLI, UFECLI, CGECLI, INECLI, INFLI' +
        'Q, INFBRT, '
      
        '   ALTVOL, LOTNFE, SEQNFE, DTENFE, RECNFE, PRONFE, DOPNFE, HRENF' +
        'E, USUNFE, '
      
        '   DTEPNF, HREPNF, IMPNFE, RETNFE, NFEPIS, NFECOF, FLGATU, ID_FI' +
        'NUFF, ID_FINCIF, '
      
        '   ID_FINUFE, ID_FINCIE, ID_TRAUFE, ID_TRACIE, ID_ESTSIP, LOCEMB' +
        ', UFEMB, '
      '   FLGDENEGADA, ENVDPEC, USUDPEC, JUSTDPEC, PROTDPEC)'
      'values'
      
        '  (:ENVNFE, :CODCF1, :CODCF2, :FLGSAI, :FLGENT, :TIPFRT, :ESPGER' +
        ', :MARGER, '
      
        '   :DESNAT, :INSSUB, :CODTRA, :NOMTRA, :CGCTRA, :INSTRA, :TENTRA' +
        ', :ENDTRA, '
      
        '   :REFTRA, :NUMTRA, :BAITRA, :CIDTRA, :UFETRA, :CEPTRA, :NROGER' +
        ', :PLCTRA, '
      
        '   :UFEPLC, :CEFCLI, :TEFCLI, :ENFCLI, :RFFCLI, :NRFCLI, :BAFCLI' +
        ', :CIFCLI, '
      
        '   :UFFCLI, :CEPCLI, :TENCLI, :ENDCLI, :REFCLI, :NUMCLI, :BAICLI' +
        ', :CIDCLI, '
      
        '   :UFECLI, :CGECLI, :INECLI, :INFLIQ, :INFBRT, :ALTVOL, :LOTNFE' +
        ', :SEQNFE, '
      
        '   :DTENFE, :RECNFE, :PRONFE, :DOPNFE, :HRENFE, :USUNFE, :DTEPNF' +
        ', :HREPNF, '
      
        '   :IMPNFE, :RETNFE, :NFEPIS, :NFECOF, :FLGATU, :ID_FINUFF, :ID_' +
        'FINCIF, '
      
        '   :ID_FINUFE, :ID_FINCIE, :ID_TRAUFE, :ID_TRACIE, :ID_ESTSIP, :' +
        'LOCEMB, '
      
        '   :UFEMB, :FLGDENEGADA, :ENVDPEC, :USUDPEC, :JUSTDPEC, :PROTDPE' +
        'C)')
