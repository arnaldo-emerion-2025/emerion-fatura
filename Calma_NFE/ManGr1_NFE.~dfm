  object FatGe2: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    DataSource = DsFatGer
    SQL.Strings = (
      'Select FatGe2.CodEmp,'
      '           FatGe2.DteGer,'
      '           FatGe2.NumGer,'
      '           FatGe2.SeqGe2,'
      '           FatGe2.DesGe2,'
      '           FatGe2.ObsGe2,'
      '           FatGe2.QtpGe2,'
      '           FatGe2.VluGe2,'
      '           FatGe2.IpiGe2,'
      '           FatGe2.IcmGe2,'
      '           FatGe2.TotIte,'
      '           FatGe2.TotIpi,'
      '           FatGe2.CodCfo,'
      '           FatGe2.ClsIpi,'
      '           FatGe2.CodUnd,'
      '           FatGe2.CodSt1,'
      '           FatGe2.CodSt2,'
      '           FatGe2.NroGe2,'
      
        '           FatGe2.CodClp || _UNICODE_FSS '#39'-'#39' || FatGe2.CodGru ||' +
        ' _UNICODE_FSS '#39'.'#39' || FatGe2.CodSub || _UNICODE_FSS '#39'.'#39' || FatGe2' +
        '.CodPro as CodIte'
      'From FatGe2'
      'Where FatGe2.CodEmp = :CodEmp'
      '     and FatGe2.DteGer = :DteGer'
      '     and FatGe2.NumGer = :NumGer'
      'Order by FatGe2.NroGe2')
