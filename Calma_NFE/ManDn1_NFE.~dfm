  object FatDev: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      
        'Select  cast(FatDev.NfsCli as integer)NFSCLI, Fatdev.* From FatD' +
        'ev'
      'Where FatDev.CodEmp = :CodEmp'
      '     and FatDev.DteRes = :DteRes'
      '     and FatDev.NumRes = :NumRes'
      '     and FatDev.SeqLib = :SeqLib'
      '     and FatDev.SeqFat = :SeqFat'
      '     and FatDev.SeqDev = :SeqDev')
    UpdateObject = UpFatDev
    ValidateWithMask = True
    Left = 21
    Top = 188
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CodEmp'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DteRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NumRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqLib'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqFat'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqDev'
        ParamType = ptInput
      end>
