        '   :BSIPIF, :BAIPIF, :TOIPIF, :IPISEG, :BSIPIS, :BAIPIS, :TOIPIS' +
        ', :IPIDES, '
      
        '   :BSIPID, :BAIPID, :TOIPID, :TOTGER, :BASCOM, :TOTCOM, :TOTDSR' +
        ', :DESREG, '
      
        '   :CODUSU, :ATUEST, :LANEST, :INTFIN, :CONSUM, :FLGCTB, :CODIPI' +
        ', :TIPIPI, '
      
        '   :TRBIPI, :REDIPI, :BSCIPI, :CODICM, :TIPICM, :TRBICM, :REDICM' +
        ', :BSCICM, '
      
        '   :INCREV, :INCFIN, :OBSMDV, :SEQREG, :FLGDIF, :FLGDEV, :FLGATU' +
        ', :SITDEV, '
      
        '   :CODCLI, :CODVEN, :MODPFA, :FLGNFS, :HRCDEV, :DTCDEV, :USUCFT' +
        ', :OBSCFT, '
      
        '   :FLGSIN, :FLGFSC, :SERNOT, :MODDEV, :OB1DEV, :OB2DEV, :OB3DEV' +
        ', :OB4DEV, '
      
        '   :OB5DEV, :OB6DEV, :OB7DEV, :OB8DEV, :FLGIMP, :NOMENT, :QTDDSE' +
        ', :SEQDSE, '
      
        '   :TOTDSE, :BASISS, :TOTISS, :TOTFAT, :FLGTOT, :BASCAT, :TOTCAT' +
        ', :FLGENV, '
      
        '   :CODTCL, :ARQNFE, :BAFCLI, :CEFCLI, :CIFCLI, :CODTRA, :DOPNFE' +
        ', :DTCNFE, '
      
        '   :DTECNE, :DTENFE, :DTEPNF, :ENFCLI, :ENVNFE, :FLGNFE, :FLGNOT' +
        ', :HRCNFE, '
      
        '   :HRECNE, :HRENFE, :HREPNF, :ID_ESTSIP, :ID_FINCIE, :ID_FINCIF' +
        ', :ID_FINUFE, '
      
        '   :ID_FINUFF, :ID_TRACIE, :ID_TRAUFE, :IMPNFE, :LOTNFE, :NFECOF' +
        ', :NFEPIS, '
      
        '   :NRFCLI, :PRCNFE, :PRONFE, :RECNFE, :RETNFE, :RFFCLI, :SEQNFE' +
        ', :TEFCLI, '
      
        '   :TRBCOF, :TRBPIS, :UFFCLI, :USUNFE, :DTEFAT, :FLGEMI, :ALTVOL' +
        ', :NFETH, '
      
        '   :ARQNFECANC, :NFETHCANC, :DTECAN, :HRECAN, :USUCAN, :OB1CAN, ' +
        ':OB2CAN, '
      '   :OB3CAN, :OB4CAN, :OB5CAN)')
    DeleteSQL.Strings = (
      'delete from FatDev'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT and'
      '  SEQDEV = :OLD_SEQDEV')
    Left = 93
    Top = 188
  end
  object FatDv2: TwwQuery
    Active = True
    CachedUpdates = True
    DatabaseName = 'ISade'
    DataSource = DsFatDev
    SQL.Strings = (
      'Select FatDv2.DesDv2,'
      '           FatDv2.CodCfo,'
      '           FatDv2.ClsIpi,'
      '           FatDv2.CodSt1,'
      '           FatDv2.CodSt2,'
      '           FatDv2.CodUnd,'
      '           FatDv2.UltQtd,'
      '           FatDv2.VlqDv2,'
      '           FatDv2.IpiDv2,'
      '           FatDv2.IcmDv2,'
      '           FatDv2.TotIpi,'
      '           FatDv2.TotDv2,'
      '           FatDv2.NroDv2,'
      
        '           FatDv2.CodClp || _UNICODE_FSS '#39'-'#39' || FatDv2.CodGru ||' +
        ' _UNICODE_FSS '#39'.'#39' || FatDv2.CodSub || _UNICODE_FSS '#39'.'#39' || FatDv2' +
        '.CodPro as CodIte'
      'From FatDv2'
      'Where FatDv2.CodEmp = :CodEmp'
      '     and FatDv2.DteRes = :DteRes'
      '     and FatDv2.NumRes = :NumRes'
      '     and FatDv2.SeqLib = :SeqLib'
      '     and FatDv2.SeqFat = :SeqFat'
      '     and FatDv2.SeqDev = :SeqDev'
      'Order by FatDv2.NroDv2')
    ValidateWithMask = True
    Left = 21
    Top = 232
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CODEMP'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DTERES'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NUMRES'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SEQLIB'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SEQFAT'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SEQDEV'
        ParamType = ptInput
      end>
    object FatDv2CODITE: TStringField
      Alignment = taCenter
      DisplayLabel = ' '
      DisplayWidth = 15
      FieldName = 'CODITE'
      Size = 16
    end
