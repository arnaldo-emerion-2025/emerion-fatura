  object UpRes: TUpdateSQL
    ModifySQL.Strings = (
      'update PedRes'
      'set'
      '  FLGEST = :FLGEST,'
      '  QTDIMP = :QTDIMP,'
      '  FLGATU = :FLGATU,'
      '  FLGIMP = :FLGIMP,'
      '  CODEAN = :CODEAN,'
      '  SITRES = :SITRES'
      'where'
      '  ID_PEDRES = :OLD_ID_PEDRES')
    InsertSQL.Strings = (
      'insert into PedRes'
      '  (ID_PEDRES, DTERES, UFERES, HRERES, DTFRES, CODPFA, TIPPFA, '
      'CODVEN, CODATD, '
      
        '   CODCLI, CODTRA, CODFIL, LANEST, FLGEST, QTIRE2, QTDIMP, FLGAT' +
        'U, '
      'TOTRES, '
      
        '   TOTIPI, TOTGER, SLDGER, FLGIMP, FLGANT, CODEAN, ID_SEPPED, SI' +
        'TRES)'
      'values'
      
        '  (:ID_PEDRES, :DTERES, :UFERES, :HRERES, :DTFRES, :CODPFA, :TIP' +
        'PFA, '
      ':CODVEN, '
      
        '   :CODATD, :CODCLI, :CODTRA, :CODFIL, :LANEST, :FLGEST, :QTIRE2' +
        ', :QTDIMP, '
      
        '   :FLGATU, :TOTRES, :TOTIPI, :TOTGER, :SLDGER, :FLGIMP, :FLGANT' +
        ', '
      ':CODEAN, '
      '   :ID_SEPPED, :SITRES)')
    DeleteSQL.Strings = (
      'delete from PedRes'
      'where'
      '  ID_PEDRES = :OLD_ID_PEDRES')
    Left = 4
    Top = 209
  end
