  object PedLb2: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select CmpR14.*,'
      
        '           TextoOcor(IntStrZeros(CmpR14.CodVen,7),'#39'-'#39',FinVen.Ape' +
        'Ven,'#39#39','#39#39','#39#39','#39#39') as ApeVen,'
      
        '           TextoOcor(IntStrZeros(CmpR14.CodCli,7),'#39'-'#39',FinCli.Nom' +
        'Cli,'#39#39','#39#39','#39#39','#39#39') as NomCli,'
      
        '           TextoOcor(CmpR14.CodGru,'#39'.'#39',CmpR14.CodSub,'#39'.'#39',CmpR14.' +
        'CodPro,'#39'-'#39',EstPro.DscPro) as CodIte'
      'From CmpR14 LEFT JOIN EstPro ON (CmpR14.CodClp = EstPro.CodClp'
      
        '                                                  AND CmpR14.Cod' +
        'Gru = EstPro.CodGru'
      
        '                                                  AND CmpR14.Cod' +
        'Sub = EstPro.CodSub'
      
        '                                                  AND CmpR14.Cod' +
        'Pro = EstPro.CodPro)'
      
        '                      LEFT JOIN FinVen ON (CmpR14.CodVen = FinVe' +
        'n.CodVen)'
      
        '                        LEFT JOIN FinCli ON (CmpR14.CodCli = Fin' +
        'Cli.CodCli)'
      'Where CmpR14.DteFat = :DteFat')
    ValidateWithMask = True
    Left = 3
    Top = 358
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'DteFat'
        ParamType = ptInput
      end>
