  object FatBol: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select FatBol.*,'
      '           FinBan.NomBan,'
      '           FinCli.NomCli,'
      '           FinCli.CecCli,'
      '           FinCli.TecCli,'
      '           FinCli.EncCli,'
      '           FinCli.NrcCli,'
      '           FinCli.RfcCli,'
      '           FinCli.BacCli,'
      '           FinCli.CicCli'
      'From FatBol,FinCli,FinBan'
      'Where FatBol.CodCli = FinCli.CodCli'
      '     and FatBol.CodBan = FinBan.CodBan'
      '     and FatBol.SeqArq = :SeqArq'
      '     and FatBol.FlgEnc = '#39'Sim'#39
      'Order by FatBol.CodBan,FatBol.DteCre,FatBol.NumCre,FatBol.NumCrp'
      '')
    UpdateObject = UpFatBol
    ControlType.Strings = (
      'FLGIMP;CheckBox;Sim;Nao')
    ValidateWithMask = True
    Left = 5
    Top = 355
    ParamData = <
      item
        DataType = ftString
        Name = 'SeqArq'
        ParamType = ptInput
      end>
