  object UpFatBol: TUpdateSQL
    ModifySQL.Strings = (
      'update FatBol'
      'set'
      '  QTPCRE = :QTPCRE,'
      '  CODCLI = :CODCLI,'
      '  CODBAN = :CODBAN,'
      '  DTVCRP = :DTVCRP,'
      '  VLPCRP = :VLPCRP,'
      '  NOSBOL = :NOSBOL,'
      '  DTVBOL = :DTVBOL,'
      '  VLPBOL = :VLPBOL,'
      '  SEQBOL = :SEQBOL,'
      '  CODTCO = :CODTCO,'
      '  CODAGB = :CODAGB,'
      '  CODCNB = :CODCNB,'
      '  NROCON = :NROCON,'
      '  NROCTR = :NROCTR,'
      '  NROAUT = :NROAUT,'
      '  FLGENC = :FLGENC,'
      '  FLGIMP = :FLGIMP,'
      '  FLGEMI = :FLGEMI,'
      '  FLGALT = :FLGALT,'
      '  NOSNUM = :NOSNUM,'
      '  CODBAR = :CODBAR,'
      '  LINDIG = :LINDIG'
      'where'
      '  SEQARQ = :OLD_SEQARQ and'
      '  CODEMP = :OLD_CODEMP and'
      '  DTECRE = :OLD_DTECRE and'
      '  NUMCRE = :OLD_NUMCRE and'
      '  NUMCRP = :OLD_NUMCRP')
    InsertSQL.Strings = (
      'insert into FatBol'
      
        '  (SEQARQ, CODEMP, DTECRE, NUMCRE, NUMCRP, QTPCRE, CODCLI, CODBA' +
        'N, '
      'DTVCRP, '
      
        '   VLPCRP, NOSBOL, DTVBOL, VLPBOL, SEQBOL, CODTCO, CODAGB, CODCN' +
        'B, '
      'NROCON, '
      
        '   NROCTR, NROAUT, FLGENC, FLGIMP, FLGEMI, FLGALT, NOSNUM, CODBA' +
        'R, '
      'LINDIG)'
      'values'
      
        '  (:SEQARQ, :CODEMP, :DTECRE, :NUMCRE, :NUMCRP, :QTPCRE, :CODCLI' +
        ', '
      ':CODBAN, '
      
        '   :DTVCRP, :VLPCRP, :NOSBOL, :DTVBOL, :VLPBOL, :SEQBOL, :CODTCO' +
        ', '
      ':CODAGB, '
      
        '   :CODCNB, :NROCON, :NROCTR, :NROAUT, :FLGENC, :FLGIMP, :FLGEMI' +
        ', '
      ':FLGALT, '
      '   :NOSNUM, :CODBAR, :LINDIG)')
    DeleteSQL.Strings = (
      'delete from FatBol'
      'where'
      '  SEQARQ = :OLD_SEQARQ and'
      '  CODEMP = :OLD_CODEMP and'
      '  DTECRE = :OLD_DTECRE and'
      '  NUMCRE = :OLD_NUMCRE and'
      '  NUMCRP = :OLD_NUMCRP')
    Left = 33
    Top = 383
  end
  object PopupMenu1: TPopupMenu
    Left = 61
    Top = 355
    object IncluirNovo1: TMenuItem
      Caption = 'Selecionar todos'
      Hint = 'Selecionar todos'
      OnClick = IncluirNovo1Click
    end
    object ExcluirAmbiente1: TMenuItem
      Caption = 'Desmarcar todos'
      Hint = 'Desmarcar todos'
      OnClick = ExcluirAmbiente1Click
    end
  end
end
