  object FatDep: TwwQuery
    Active = True
    CachedUpdates = True
    OnNewRecord = FatDepNewRecord
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select FatDep.*,'
      '           PedRes.CodCli,'
      '           FinCli.ApeCli as NomCli,'
      '           PedRes.CodVen,'
      '           PedRes.UfeRes,'
      '           FinVen.ApeVen,'
      '           FinVen.NomVen,'
      '           GerUsu.LogUsu'
      'From FatDep INNER JOIN PedLib ON (FatDep.CodEmp = PedLib.CodEmp)'
      
        '                                                    AND (FatDep.' +
        'DteRes = PedLib.DteRes)'
      
        '                                                    AND (FatDep.' +
        'NumRes = PedLib.NumRes)'
      
        '                                                    AND (FatDep.' +
        'SeqLib = PedLib.SeqLib)'
      
        '                      LEFT JOIN PedRes ON (PedLib.CodEmp = PedRe' +
        's.CodEmp)'
      
        '                                                    AND (PedLib.' +
        'DteRes = PedRes.DteRes)'
      
        '                                                    AND (PedLib.' +
        'NumRes = PedRes.NumRes)'
      
        '                      LEFT JOIN FinCli ON (PedRes.CodCli = FinCl' +
        'i.CodCli)'
      
        '                      LEFT JOIN FinVen ON (PedRes.CodVen = FinVe' +
        'n.CodVen)'
      
        '                      LEFT JOIN EstPfa ON (PedRes.CodPfa = EstPf' +
        'a.CodPfa)'
      
        '                                                  AND  (PedRes.T' +
        'ipPfa = EstPfa.TipPfa)'
      
        '                      LEFT JOIN GerUsu ON (FatDep.CodUsu = GerUs' +
        'u.CodUsu)'
      'Where FatDep.CodEmp = :CodEmp'
      '     and FatDep.DteRes = :DteRes'
      '     and FatDep.NumRes = :NumRes'
      '     and FatDep.SeqLib = :SeqLib'
      '     and FatDep.SeqDep = :SeqDep')
    UpdateObject = UpDep
    ValidateWithMask = True
    Left = 136
    Top = 244
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CodEmp'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DteRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NumRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqLib'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqDep'
        ParamType = ptInput
      end>
    object FatDepCODEMP: TIntegerField
      FieldName = 'CODEMP'
    end
