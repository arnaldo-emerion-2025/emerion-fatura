  object UpDep: TUpdateSQL
    ModifySQL.Strings = (
      'update FatDep'
      'set'
      '  DTEDEP = :DTEDEP,'
      '  HREDEP = :HREDEP,'
      '  QTIDEP = :QTIDEP,'
      '  SEQITE = :SEQITE,'
      '  QTSDEP = :QTSDEP,'
      '  TOTDEP = :TOTDEP,'
      '  TOTGER = :TOTGER,'
      '  BASCOM = :BASCOM,'
      '  TOTCOM = :TOTCOM,'
      '  CODUSU = :CODUSU,'
      '  FLGDEP = :FLGDEP,'
      '  OBSDEP = :OBSDEP,'
      '  SEQREG = :SEQREG,'
      '  SITDEP = :SITDEP'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQDEP = :OLD_SEQDEP')
    InsertSQL.Strings = (
      'insert into FatDep'
      
        '  (CODEMP, DTERES, NUMRES, SEQLIB, SEQDEP, DTEDEP, HREDEP, QTIDE' +
        'P, '
      'SEQITE, '
      
        '   QTSDEP, TOTDEP, TOTGER, BASCOM, TOTCOM, CODUSU, FLGDEP, OBSDE' +
        'P, '
      'SEQREG, '
      '   SITDEP)'
      'values'
      
        '  (:CODEMP, :DTERES, :NUMRES, :SEQLIB, :SEQDEP, :DTEDEP, :HREDEP' +
        ', '
      ':QTIDEP, '
      
        '   :SEQITE, :QTSDEP, :TOTDEP, :TOTGER, :BASCOM, :TOTCOM, :CODUSU' +
        ', '
      ':FLGDEP, '
      '   :OBSDEP, :SEQREG, :SITDEP)')
    DeleteSQL.Strings = (
      'delete from FatDep'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQDEP = :OLD_SEQDEP')
    Left = 136
    Top = 300
  end
