  object FatArq: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select FatPed.CodEmp,'
      '           FatPed.DteRes,'
      '           FatPed.NumRes,'
      '           FatPed.SeqLib,'
      '           FatPed.SeqFat,'
      '           FatPed.ArqNfe,'
      '           FatPed.FlgAtu'
      'From FatPed'
      'Where FatPed.CodEmp = :CodEmp'
      '     and FatPed.DteRes = :DteRes'
      '     and FatPed.NumRes = :NumRes'
      '     and FatPed.SeqLib = :SeqLib'
      '     and FatPed.SeqFat = :SeqFat')
    UpdateObject = UpFatArq
    ValidateWithMask = True
    Left = 65
    Top = 192
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CodEmp'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DteRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NumRes'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqLib'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'SeqFat'
        ParamType = ptInput
      end>
    object FatArqARQNFE: TBlobField
      FieldName = 'ARQNFE'
      BlobType = ftBlob
      Size = 1
    end
    object FatArqFLGATU: TStringField
