  object FatPe2: TwwQuery
    Active = True
    CachedUpdates = True
    DatabaseName = 'ISade'
    DataSource = DsFatPed
    SQL.Strings = (
      'Select FatPe2.CodEmp,'
      '           FatPe2.DteRes,'
      '           FatPe2.NumRes,'
      '           FatPe2.SeqLib,'
      '           FatPe2.SeqFat,'
      '           FatPe2.SeqPe2,'
