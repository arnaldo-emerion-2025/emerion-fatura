  object UpFatArq: TUpdateSQL
    ModifySQL.Strings = (
      'update FatPed'
      'set'
      '  ARQNFE = :ARQNFE,'
      '  FLGATU = :FLGATU'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    InsertSQL.Strings = (
      'insert into FatPed'
      '  (CODEMP, DTERES, NUMRES, SEQLIB, SEQFAT, ARQNFE, FLGATU)'
      'values'
      
        '  (:CODEMP, :DTERES, :NUMRES, :SEQLIB, :SEQFAT, :ARQNFE, :FLGATU' +
        ')')
    DeleteSQL.Strings = (
      'delete from FatPed'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQLIB = :OLD_SEQLIB and'
      '  SEQFAT = :OLD_SEQFAT')
    Left = 93
    Top = 192
  end
  object UpFatPe2: TUpdateSQL
    ModifySQL.Strings = (
      'update FatPe2'
